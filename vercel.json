{
	"version": 2,
	"builds": [
		{
			"src": "index.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": [
					"dist/**"
				]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.ts"
		}
	],
	"functions": {
		"pages/api/resetLeaderboard.ts": {
			"runtime": "nodejs18.x"
		}
	}
}